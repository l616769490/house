            <yAxes>
                <yAxis name="户数"/>
            </yAxes>
            <!-- x轴 -->
            <xAxes>
                <xAxis>
                    <name>分类</name>
                    <dimGroupName>收入统计</dimGroupName>
                </xAxis>
            </xAxes>
            <!-- 图例 -->
            <legends>
                <legend>
                    <name>区域划分</name>
                    <dimGroupName>区域</dimGroupName>
                </legend>
            </legends>
        </diagram>
    </diagrams>
</report>